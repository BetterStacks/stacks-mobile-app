{
  "expo": {
    "name": "Stacks",
    "slug": "stacks",
    "version": "3.0.0",
    "orientation": "portrait",
    "icon": "./assets/png/stacks-android-icon.png",
    "scheme": "betterstacks",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.betterstacks",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/png/stacks-android-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.betterstacks",
      "googleServicesFile": "./google-services.json",
      "permissions": []
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/png/stacks-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-share-intent",
      [
        "expo-splash-screen",
        {
          "image": "./assets/png/stacks-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-google-signin/google-signin",
      "react-native-video",
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "build": {
          "experimental": {
            "ios": {}
          }
        },
        "projectId": "812e3222-ced8-4683-8628-7ecc1d558da5"
      }
    },
    "owner": "betterstacks"
  }
}
